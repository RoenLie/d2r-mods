{
	"editor.bracketPairColorization.enabled": true,
	"editor.guides.bracketPairs": "active",
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"eslint.run": "onType",
	"eslint.useESLintClass": true,
	"eslint.useFlatConfig": true,
	"eslint.format.enable": true,
	"eslint.validate": [
		"javascript",
		"typescript",
		"typescriptreact"
	],
	"eslint.workingDirectories": [
		{
			"mode": "auto"
		}
	],
	"[javascript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[typescript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"typescript.format.insertSpaceAfterTypeAssertion": true,
	"typescript.format.semicolons": "insert",
	"javascript.format.semicolons": "insert",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"typescript.preferences.importModuleSpecifier": "project-relative",
	"javascript.preferences.importModuleSpecifier": "project-relative",
	"typescript.preferences.importModuleSpecifierEnding": "js",
	"javascript.preferences.importModuleSpecifierEnding": "js",
	"typescript.preferences.quoteStyle": "single",
	"javascript.preferences.quoteStyle": "single",
	"typescript.tsdk": "node_modules\\typescript\\lib",
	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#463179",
		"activityBar.background": "#463179",
		"activityBar.foreground": "#e7e7e7",
		"activityBar.inactiveForeground": "#e7e7e799",
		"activityBarBadge.background": "#9f5c40",
		"activityBarBadge.foreground": "#e7e7e7",
		"commandCenter.border": "#e7e7e799",
		"sash.hoverBorder": "#463179",
		"statusBar.background": "#312255",
		"statusBar.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#463179",
		"statusBarItem.remoteBackground": "#312255",
		"statusBarItem.remoteForeground": "#e7e7e7",
		"titleBar.activeBackground": "#312255",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveBackground": "#31225599",
		"titleBar.inactiveForeground": "#e7e7e799"
	},
	"peacock.color": "#312255",
	"restoreTerminals.runOnStartup": false,
	"restoreTerminals.terminals": [
		{
			"splitTerminals": [
				{
					"name": "root",
					"commands": [
						"clear"
					]
				}
			]
		},
	],
	"workbench.editorAssociations": {
		"*.copilotmd": "vscode.markdown.preview.editor",
		"*.ipynb": "jupyter-notebook",
		"*.db": "sqlite-viewer.option",
		"*.svg": "default",
		"*.sprite": "d2rmodding.spriteViewer"
	}
}
